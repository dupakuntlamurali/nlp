import spacy

nlp = spacy.load("en_core_web_sm")
sentence = "she eats an apple."
doc = nlp(sentence)
subcat_frames = {}

for token in doc:
    if token.dep_ == "dobj":
        verb = token.head.text
        obj = token.text
        if verb not in subcat_frames:
            subcat_frames[verb] = []
        subcat_frames[verb].append(obj)

for verb, objects in subcat_frames.items():
